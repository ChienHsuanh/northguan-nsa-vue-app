{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/northguan-nsa-vue-app.Server/northguan-nsa-vue-app.Server.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "options": {
        "cwd": "${workspaceFolder}/northguan-nsa-vue-app.Server"
      },
      "presentation": {
        "reveal": "silent"
      }
    },
    {
      "label": "publish",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/northguan-nsa-vue-app.Server/northguan-nsa-vue-app.Server.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "start-server-debug",
      "command": "dotnet",
      "type": "process",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/northguan-nsa-vue-app.Server/northguan-nsa-vue-app.Server.csproj",
        "--",
        "--urls",
        "https://localhost:7289;http://localhost:5114"
      ],
      "group": "build",
      "isBackground": true,
      "options": {
        "cwd": "${workspaceFolder}/northguan-nsa-vue-app.Server",
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development",
          "DOTNET_ENVIRONMENT": "Development"
        }
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      }
    },
    {
      "label": "start-server-watch",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "--project",
        "${workspaceFolder}/northguan-nsa-vue-app.Server/northguan-nsa-vue-app.Server.csproj",
        "--",
        "--urls",
        "https://localhost:7289;http://localhost:5114"
      ],
      "group": "build",
      "isBackground": true,
      "options": {
        "cwd": "${workspaceFolder}/northguan-nsa-vue-app.Server",
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development",
          "DOTNET_ENVIRONMENT": "Development"
        }
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      }
    },
    {
      "label": "restore",
      "command": "dotnet",
      "type": "process",
      "args": [
        "restore",
        "${workspaceFolder}/northguan-nsa-vue-app.Server/northguan-nsa-vue-app.Server.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "clean",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "${workspaceFolder}/northguan-nsa-vue-app.Server/northguan-nsa-vue-app.Server.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "npm install (client)",
      "type": "shell",
      "command": "npm",
      "args": ["install"],
      "options": {
        "cwd": "${workspaceFolder}/northguan-nsa-vue-app.client"
      },
      "group": "build",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    },
    {
      "label": "npm run dev (client)",
      "type": "shell",
      "command": "npm",
      "args": ["run", "dev"],
      "options": {
        "cwd": "${workspaceFolder}/northguan-nsa-vue-app.client"
      },
      "group": "build",
      "isBackground": true,
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "problemMatcher": {
        "owner": "typescript",
        "source": "ts",
        "applyTo": "closedDocuments",
        "fileLocation": [
          "relative",
          "${workspaceFolder}/northguan-nsa-vue-app.client"
        ],
        "pattern": "$tsc",
        "background": {
          "activeOnStart": true,
          "beginsPattern": {
            "regexp": "(.*?)"
          },
          "endsPattern": {
            "regexp": "Local:.*"
          }
        }
      }
    },
    {
      "label": "npm run build (client)",
      "type": "shell",
      "command": "npm",
      "args": ["run", "build"],
      "options": {
        "cwd": "${workspaceFolder}/northguan-nsa-vue-app.client"
      },
      "group": "build",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    },
    {
      "label": "Build Full Stack",
      "dependsOrder": "sequence",
      "dependsOn": ["restore", "npm install (client)", "build"],
      "group": {
        "kind": "build",
        "isDefault": false
      }
    },
    {
      "label": "Build and Run Development",
      "dependsOrder": "sequence",
      "dependsOn": ["Build Full Stack", "npm run dev (client)"],
      "group": "build"
    },
    {
      "label": "watch-debug",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "--project",
        "${workspaceFolder}/northguan-nsa-vue-app.Server/northguan-nsa-vue-app.Server.csproj",
        "--",
        "--urls",
        "https://localhost:7289;http://localhost:5114"
      ],
      "problemMatcher": {
        "owner": "dotnet",
        "source": "dotnet",
        "fileLocation": "absolute",
        "pattern": [
          {
            "regexp": "^.*\\s+(.*)\\((\\d+),(\\d+)\\):\\s+(error|warning|info)\\s+(\\w{1,2}\\d+):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "code": 5,
            "message": 6
          }
        ],
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^.*dotnet watch|^.*watch : Started|^.*Building\\.\\.\\.",
          "endsPattern": "^.*Application started\\. Press Ctrl\\+C to shut down\\.|^.*Now listening on:|^.*Content root path:"
        }
      },
      "group": "build",
      "isBackground": true,
      "options": {
        "cwd": "${workspaceFolder}/northguan-nsa-vue-app.Server",
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development",
          "DOTNET_WATCH_RESTART_ON_RUDE_EDIT": "true",
          "DOTNET_WATCH_SUPPRESS_LAUNCH_BROWSER": "true"
        }
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": true,
        "clear": false
      }
    },
    {
      "label": "clean-then-build",
      "dependsOrder": "sequence",
      "dependsOn": ["clean", "build"],
      "group": {
        "kind": "build",
        "isDefault": false
      }
    }
  ]
}
